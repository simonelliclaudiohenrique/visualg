Algoritmo "semnome"

Var
    distancia,combustivel,consMed:real


Inicio
       escreva("Distância percorrida: ")
       leia(distancia)
       escreva("Combustível gasto: ")
       leia(combustivel)
       
       consMed <- distancia / combustivel
       
       escreva("Consumo Médio: ",consMed:5:3)

Fimalgoritmo